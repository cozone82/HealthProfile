<section class="position-relative py-4 py-xl-5">
    <div class="container">
        <div class="row mb-5">
            <div class="col-md-8 col-xl-12 text-center mx-auto">
                <h4>Results</h4>
            </div>
        </div>
        <div class="row d-flex justify-content-center">
            <div class="col-md-6 col-xl-4">
                <div class="card mb-5">
                    <div class="card-body d-flex flex-column align-items-center">
                        <div>
                            <label>Full Name:</label>
                            <input value="@ViewBag.FullName" class="form-control" readonly/>
                        </div>
                        <div>
                            <label>Age:</label>
                            <input value="@ViewBag.Age" class="form-control" readonly/>
                        </div>
                        <div>
                            <label>Gender:</label>
                            <input value="@ViewBag.Gender" class="form-control" readonly/>
                        </div>
                         <div>
                            <label>BMI:</label>
                            <input value="@ViewBag.BMI" class="form-control" readonly/>
                        </div>
                        <div>
                            <label>Max Heart Rate:</label>
                            <input value="@ViewBag.MaximumHeartRate" class="form-control" readonly/>
                        </div>
                        <div>
                            <label>Min Target Heart Rate:</label>
                            <input value="@ViewBag.MinimumTargetHeartRate" class="form-control" readonly/>
                        </div>
                         <div>
                            <label>Max target Heart Rate:</label>
                            <input value="@ViewBag.MaximumTargetHeartRate" class="form-control" readonly/>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
